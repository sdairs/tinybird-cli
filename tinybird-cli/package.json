{
  "name": "tinybird-cli",
  "description": "CLI for Tinybird",
  "version": "0.0.0",
  "author": "sdairs",
  "bin": {
    "tinybird-cli": "./bin/run.js"
  },
  "bugs": "https://github.com/sdairs/tinybird-cli/issues",
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "@oclif/core": "^4.0.34",
    "@oclif/plugin-help": "^6.2.18",
    "@oclif/plugin-plugins": "^5.4.20",
    "@sdairs/tinybird-markdoc-ext": "file:../tinybird-markdoc-ext",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/react": "^18",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "inquirer": "^12.1.0",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.2",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.67",
    "chai": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-config-oclif": "^5.2.2",
    "eslint-config-oclif-typescript": "^3.1.12",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.8.2",
    "oclif": "^4.16.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sdairs/tinybird-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GNU GPL v3",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "tinybird-cli",
    "dirname": "tinybird-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "sdairs/tinybird-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}